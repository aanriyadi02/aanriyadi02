.nav-links-wrapper { align-self: flex-start; }

.nav, .nav-links-wrapper { display: flex; transition: 1s cubic-bezier(0.19, 1, 0.22, 1); }

.nav { position: absolute; z-index: 1000; margin: 0px 0px 0px -12px; padding: 0px; list-style: none; }

.nav li { display: block; margin: 0px; padding: 0px; }

